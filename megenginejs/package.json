{
  "name": "megenginejs",
  "version": "0.2.1",
  "types": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "license": "Apache-2.0",
  "homepage": "https://github.com/MediosZ/MegEngine/tree/megjs",
  "scripts": {
    "build-mt": "node ./scripts/patch-worker-module.js && node ./scripts/patch-threaded-simd-module.js && yarn rollup -c rollup.config.js",
    "build": "yarn rollup -c rollup.config.js && cp wasm-out/meg* dist/",
    "test": "yarn karma start"
  },
  "browser": {
    "fs": false,
    "path": false,
    "worker_threads": false,
    "perf_hooks": false
  },
  "staticFiles": {
    "staticPath": "./wasm-out",
    "excludeGlob": [
      "**/!(*.wasm)"
    ]
  },
  "dependencies": {
    "@types/emscripten": "0.0.34",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^27.0.1",
    "jasmine-core": "^3.7.1",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-typescript": "^5.5.1",
    "karma-typescript-es6-transform": "^5.5.1",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "rollup": "^2.48.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.2.0"
  }
}
